{
  "name": "tabletune",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint .  --ext .js --ext .jsx --ext .ts --ext .tsx --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "preview": "vite preview",
    "build-css": "sass src/",
    "watch-css": "npm run build-css && sass src/ --watch",
    "typecheck:prod": "tsc -p tsconfig.json --noEmit",
    "tsc:diag": "tsc --extendedDiagnostics"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/node": "^24.3.0",
    "@vite-pwa/assets-generator": "^1.0.0",
    "babel-loader": "^10.0.0",
    "eslint-plugin-jest": "^29.0.1",
    "react": "^19.1.1",
    "react-datepicker": "^8.5.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.0",
    "react-toastify": "^11.0.5",
    "redux-persist": "^6.0.0",
    "router": "^2.2.0",
    "sass": "^1.90.0",
    "vite-plugin-pwa": "^1.0.2",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
